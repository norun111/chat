.chat-side
  .side-bar
    .side-bar__user-name
      .top-items
        .top-items__name
          = current_user.name
        .top-items__icons
          = link_to "#", class:"top-items__icons--edit"  do
            = icon('fas','edit')
          = link_to edit_user_path(current_user) do
            = icon('fas', 'cog', class: 'icon')
    .side-bar__user-list
      .side-bar__user-list-group-name
        techexpert
      .side-bar__user-list-message
        nothing